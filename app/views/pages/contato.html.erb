<section class="probootstrap-section contact" style="background-image:linear-gradient(to left, rgba(9,88,93, 1) 50%, rgba(9, 88, 93, .1) 75%, transparent 50%),url(<%= image_url('back_contact.jpeg')%>);margin-top:64px">
    <!-- <div class="golden-border "></div> -->
    <div class="container">
      <div class="row">
        
        <div class="col-md-12" style="margin:30px 0;">
        	<h2 class="contact-title"> <b>Contato</b></h2>
        </div>
        <div class="col-md-4 col-md-offset-6">
        	<form class="contact-form">
        		<input placeholder="Nome" type="text">
        		<input placeholder="Email" type="email">
        		<input placeholder="Telefone(opcional)" type="text">
        		<input placeholder="Assunto" type="text">
        		<textarea placeholder="Mensagem..."></textarea>
        	</form>
        </div>       
      </div>
    </div>
</section>



